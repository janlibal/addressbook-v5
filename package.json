{
  "name": "addressbook-v5",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "migration:make": "knex migrate:make --knexfile ./src/database/knexfile.ts",
    "db-dev:migrate": "knex migrate:latest --env development --knexfile ./src/database/knexfile.ts",
    "db-test:migrate": "knex migrate:latest --env test --knexfile ./src/database/knexfile.ts",
    "seed:make": "knex seed:make --knexfile ./src/database/knexfile.ts",
    "db-dev:seed": "knex seed:run --env development --knexfile ./src/database/knexfile.ts",
    "db-test:seed": "knex seed:run --env test --knexfile ./src/database/knexfile.ts",
    "dev": "cross-env ENV_FILE=./src/config/env/.env.dev nodemon",
    "prod": "cross-env ENV_FILE=./src/config/env/.env.prod nodemon",
    "staging": "cross-env ENV_FILE=./src/config/env/.env.staging ts-node-dev --poll --respawn --transpile-only ./src/index.ts",
    "coverage": "nyc report --reporter=json",
    "test": "cross-env ENV_FILE=./src/config/env/.env.test mocha -r ts-node/register --timeout 5000 --colors ./tests/**/*.spec.ts",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "lint:fix": "eslint --ignore-path .eslintignore --fix .",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/firestore": "^6.5.0",
    "@koa/cors": "^4.0.0",
    "@types/node": "^18.15.10",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "dotenv-extended": "^2.9.0",
    "dotenv-parse-variables": "^2.0.0",
    "firebase-admin": "^11.8.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "kcors": "^2.2.2",
    "knex": "^2.4.2",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.0",
    "koa-compress": "^5.1.0",
    "koa-helmet": "^7.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "nyc": "^15.1.0",
    "objection": "^3.0.1",
    "p-event": "^5.0.1",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "ramda": "^0.28.0",
    "stoppable": "^1.1.0",
    "supertest": "^6.3.3",
    "typescript": "^5.0.2",
    "util": "^0.12.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@ngneat/falso": "^6.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-parse-variables": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/kcors": "^2.2.6",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^4.0.0",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "@types/ramda": "^0.28.23",
    "@types/stoppable": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "i": "^0.3.7",
